
@model EasyMarket.Models.Supermercado

<div class="panel panel-info">
    <div class="panel-heading"> Detalhe do Supermercado </div>
    <div class="panel-body">
        <div>
                <dl class="dl-horizontal">
                            <dt>
                                Nome
                            </dt>
                            <dd>
                                @Html.DisplayFor(model => model.Nome)
                            </dd>
                            <dt>
                                Cnpj
                            </dt>
                            <dd>
                                @Html.DisplayFor(model => model.Cnpj)
                            </dd>
                        </dl>                  
            </div>
        <p>
            <a href="@Url.Action("Index", "Supermercado")" class="btn btn-primary btn-sm"> <span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span> Voltar</a> &nbsp;
            <a href="@Url.Action("Edit", "Supermercado",new { id = Model.Id })" class="btn btn-warning btn-sm"> <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> &nbsp; Editar </a>
        </p>
    </div>
</div>
